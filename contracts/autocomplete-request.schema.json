{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "autocomplete-request.schema.json",
  "title": "AutocompleteRequest",
  "type": "object",
  "additionalProperties": false,
  "required": ["q"],
  "properties": {
    "q": { "type": "string", "minLength": 1 },
    "size": { "type": "integer", "minimum": 1, "maximum": 50 }
  }
}
